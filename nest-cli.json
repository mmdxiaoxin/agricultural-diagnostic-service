{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": ["**/*.proto"],
    "watchAssets": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "diagnosis-service": {
      "type": "application",
      "root": "apps/diagnosis-service",
      "entryFile": "main",
      "sourceRoot": "apps/diagnosis-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/diagnosis-service/tsconfig.app.json"
      }
    },
    "download-service": {
      "type": "application",
      "root": "apps/download-service",
      "entryFile": "main",
      "sourceRoot": "apps/download-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/download-service/tsconfig.app.json"
      }
    },
    "file-operation": {
      "type": "library",
      "root": "libs/file-operation",
      "entryFile": "index",
      "sourceRoot": "libs/file-operation/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file-operation/tsconfig.lib.json"
      }
    },
    "mail": {
      "type": "library",
      "root": "libs/mail",
      "entryFile": "index",
      "sourceRoot": "libs/mail/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mail/tsconfig.lib.json"
      }
    },
    "metrics": {
      "type": "library",
      "root": "libs/metrics",
      "entryFile": "index",
      "sourceRoot": "libs/metrics/src",
      "compilerOptions": {
        "tsConfigPath": "libs/metrics/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "upload-service": {
      "type": "application",
      "root": "apps/upload-service",
      "entryFile": "main",
      "sourceRoot": "apps/upload-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/upload-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}
