{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/ali-oss": "<rootDir>/../libs/ali-oss/src",
    "@app/ali-oss/(.*)": "<rootDir>/../libs/ali-oss/src/$1",
    "@app/database": "<rootDir>/../libs/database/src",
    "@app/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@app/file-operation": "<rootDir>/../libs/file-operation/src",
    "@app/file-operation/(.*)": "<rootDir>/../libs/file-operation/src/$1",
    "@app/mail": "<rootDir>/../libs/mail/src",
    "@app/mail/(.*)": "<rootDir>/../libs/mail/src/$1",
    "@app/metrics": "<rootDir>/../libs/metrics/src",
    "@app/metrics/(.*)": "<rootDir>/../libs/metrics/src/$1",
    "@app/redis": "<rootDir>/../libs/redis/src",
    "@app/redis/(.*)": "<rootDir>/../libs/redis/src/$1"
  }
}